{
  "name": "makasim/values",
  "type": "library",
  "description": "Highly opinionated way of build Models",
  "keywords": ["model", "mongo"],
  "license": "MIT",
  "authors": [
    {
      "name": "Kotlyar Maksim",
      "email": "kotlyar.maksim@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": { "Makasim\\Values\\": "src" },
    "files": ["src/functions_include.php"]
  },
  "autoload0-dev": {
    "psr-4": { "Makasim\\Values\\Tests": "tests" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
